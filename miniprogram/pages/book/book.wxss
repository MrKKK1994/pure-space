.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #f5f5f5;
}

.header {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 100;
    height: 92rpx;
    background-color: #fff;
    border-top:1px solid #f5f5f5;
}

.search {
    background-color: #f5f5f5;
    margin: 16rpx 26rpx;
    border-radius: 30rpx;
    font-size: 28rpx;
    color: rgb(140, 140, 140);
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.search>image {
    height: 28rpx;
    width: 28rpx;
    margin-right: 20rpx;
}

.header-image {
    width: 106rpx;
    height: 34rpx;
    margin-top: 40rpx;
}

.book-box {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 92rpx;
}

.book-container {
    margin-top: 10rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    padding: 20rpx 90rpx 0;
    justify-content: space-between;
}

.book-container>bookCmp {
    margin-bottom: 30rpx;
}